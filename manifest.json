{
	"name": "Keeweb",
	"id": "keeweb",
	"packaging_format": 1,
	"description": {
		"en": "Password manager compatible with KeePass.",
		"fr": "Gestionnaire de mots de passe compatible avec KeePass."
	},
	"version": "1.7.4~ynh2",
	"url": "https://keeweb.info/",
	"license": "MIT",
	"maintainer": {
		"name": "Scith",
		"url": "https://github.com/scith"
	},
	"requirements": {
		"yunohost": ">= 2.7.14"
	},
	"multi_instance": true,
	"services": [
		"nginx"
	],
	"arguments": {
		"install" : [
			{
				"name": "domain",
				"type": "domain",
				"ask": {
					"en": "Choose a domain name for Keeweb",
					"fr": "Choisissez un nom de domaine pour Keeweb"
				},
				"example": "example.com"
			},
			{
				"name": "path",
				"type": "path",
				"ask": {
					"en": "Choose a path for Keeweb",
					"fr": "Choisissez un chemin pour Keeweb"
				},
				"example": "/keeweb",
				"default": "/keeweb"
			},
			{
				"name": "is_public",
				"type": "boolean",
				"ask": {
					"en": "Is it a public application?",
					"fr": "Est-ce une application publique ?"
				},
				"default": false
			}
		]
	}
}
